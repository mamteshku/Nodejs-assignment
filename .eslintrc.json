{
    "env": {
        "node": true,
        "es6": true
    },
    "plugins": ["security"],
    "extends":  [
        "plugin:security/recommended"
      ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "quotes": ["error", "double", { "allowTemplateLiterals": true }],
        "semi": ["error", "always"],
        "no-console": "off"
    }
}